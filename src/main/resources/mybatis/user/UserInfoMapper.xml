<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.middle.ccs.user.dao.UserInfoMapper">
    <select id="selectUserList" resultType="com.middle.ccs.user.entity.UserInfo" parameterType="com.middle.ccs.user.entity.UserInfo">
        select user_code, user_password from user_info where user_code = #{userCode}
    </select>

</mapper>
